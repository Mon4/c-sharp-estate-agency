<Window x:Class="WpfEstates.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfEstates"
        mc:Ignorable="d"
        Title="Main Window" Height="768" Width="1366">
    <Grid>
        <Button x:Name="EployeesBtn" Content="Eployees" HorizontalAlignment="Left" Height="66" Margin="497,256,0,0" VerticalAlignment="Top" Width="378" Click="EployeesBtn_Click"/>
        <Button x:Name="ClientsBtn" Content="Clients" HorizontalAlignment="Center" Height="66" Margin="0,338,0,0" VerticalAlignment="Top" Width="378" Click="ClientsBtn_Click"/>
        <Button x:Name="OwnersBtn" Content="Owners" HorizontalAlignment="Center" Height="66" Margin="0,420,0,0" VerticalAlignment="Top" Width="378" Click="OwnersBtn_Click"/>
        <Button x:Name="MeetingsBtn" Content="Meetings" HorizontalAlignment="Center" Height="66" Margin="0,507,0,0" VerticalAlignment="Top" Width="378" Click="MeetingsBtn_Click"/>
        <Button x:Name ="EstatesBtn" Content="Estates" HorizontalAlignment="Left" Height="66" Margin="497,176,0,0" VerticalAlignment="Top" Width="378" Click="EstatesBtn_Click"/>
        <Button x:Name="ExitBtn" Content="Exit" HorizontalAlignment="Center" Height="43" Margin="0,683,0,0" VerticalAlignment="Top" Width="278" Click="ExitBtn_Click"/>
        <Label Content="Estates Program" HorizontalAlignment="Left" Height="71" Margin="497,59,0,0" VerticalAlignment="Top" Width="375" FontSize="22"/>
    </Grid>
</Window>
